package sec01.exam01;

public class VariableInitializationExample {

	public static void main(String[] args) {
		int value = 20;
		int result = value + 10;
		System.out.println(result);
		
		int x = 3;
		int y = 5;
		System.out.println("x:" + x + ", y:" + y);
		
		int temp = x;
		x = y;
		y = temp;
		System.out.println("x:" + x + ", y:" + y);
	
		int sum = 0;
		for (int i=1; i<=10; i++) {
			sum = sum + i;
		
		int temp2 = sum;
		if(temp2>15)
		i = temp2;
		System.out.println(temp2);
		}
			
	}

}
